<script>
$(window).load(function() {
	var url = 'http://brasilescola.uol.com.br/biologia/';
	$.ajax({
		url: 'https://graph.facebook.com',
		async: true,
		type: 'GET',
		data: {
			ids: url
		},
		dataType: 'json',
		success: function (json) {
			if (json[url]) {
				$('#get_likes').text(json[url].share.share_count);
			}
		}
	});
});
</script>
